<?xml version="1.0" encoding="utf-8"?>

<!-- Definição de tipos globais do C3. -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cat="http://www.cip-bancos.org.br/catalogomsg"
           id="ARRCTIPOS" elementFormDefault="qualified" version="10.1">

<xs:annotation>
    <xs:documentation>
      Definição de tipos globais do R2C3.
    </xs:documentation>
  </xs:annotation>

  <!-- Definição do ARRCDOC do arquivo PRO -->
  <xs:complexType name="ARRCDOCPROComplexType">
    <xs:sequence>
      <xs:element name="BCARQ" type="BCARQComplexType"/>
    </xs:sequence>
  </xs:complexType>
  
  <!-- Definição do ADDADOC do arquivo ERR -->
  <xs:complexType name="ADDADOCERRComplexType">
    <xs:sequence>
      <xs:element name="BCARQ" type="BCARQERRComplexType"/>
      <xs:element name="ESTARQ" type="ESTARQSimpleType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <!-- Definição do ARRCDOC do arquivo ERR -->
  <xs:complexType name="ARRCDOCERRComplexType">
    <xs:sequence>
      <xs:element name="BCARQ" type="BCARQERRComplexType"/>
    </xs:sequence>
  </xs:complexType>


  <!-- Definicao do BCMSG da mensagem -->

  <xs:complexType name="BCMSGComplexType">
    <xs:sequence>
      <xs:element name="IdentdEmissor" type="ISPB"/>
      <xs:element name="IdentdDestinatario" type="ISPB"/>
      <xs:element name="IdentdContg" type="ISPB" minOccurs="0"/>
      <xs:element name="IdentdOperad" type="IdentdOperad" minOccurs="0"/>
      <xs:element name="IdentdOperadConfc" type="IdentdOperad" minOccurs="0"/>
      <xs:element name="Grupo_Seq" type="Grupo_SeqComplexType" minOccurs="0"/>
      <xs:element name="DomSist" type="DomSist"/>
      <xs:element name="NUOp" type="NUOp"/>
    </xs:sequence>
  </xs:complexType>

    <xs:simpleType name="NUOp">
        <xs:annotation>
            <xs:documentation>
                <cat:InfTipo>
                    <cat:DescricaoTipo>Número único da operação. Formato: para o Domínio de Sistema SPB02 = XXXXXXXXAAMMDDSSSSSSSSS em que XXXXXXXX - ISPB; AAMMDD - data; SSSSSSSSS - sequencial. Para os demais domínios de sistema = XXXXXXXXAAAAMMDDSSSSSSS em que XXXXXXXX - ISPB ou CNPJ de 8 posições; AAAAMMDD - data; SSSSSSS - sequencial.</cat:DescricaoTipo>
                </cat:InfTipo>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="23"/>
            <xs:maxLength value="23"/>
            <xs:pattern value="[0-9]{23}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DomSist">
        <xs:annotation>
            <xs:documentation>
                <cat:InfTipo>
                    <cat:DescricaoTipo>Domínio de sistema</cat:DescricaoTipo>
                </cat:InfTipo>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="5"/>
            <xs:maxLength value="5"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="IdentdOperad">
        <xs:annotation>
            <xs:documentation>
                <cat:InfTipo>
                    <cat:DescricaoTipo>Identificador pessoal do operador.</cat:DescricaoTipo>
                </cat:InfTipo>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="32"/>
        </xs:restriction>
    </xs:simpleType>

  <xs:simpleType name="CodMsg">
    <xs:annotation>
      <xs:documentation>
        <cat:InfTipo>
          <cat:DescricaoTipo>Código da mensagem do sistema associado ao evento.</cat:DescricaoTipo>
        </cat:InfTipo>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="7"/>
      <xs:maxLength value="9"/>
      <xs:pattern value="[A-Z]{3}[0-9]{4}(E|R1|R2|R3)?"/>
    </xs:restriction>
  </xs:simpleType>

    <xs:complexType name="CodMsgCodErr">
        <xs:annotation>
            <xs:documentation>
                Campo de erro para processamento.
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="CodMsg">
                <xs:attribute name="CodErro" type="CodErro" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

  <!-- Definição do BCARQ do arquivo -->
  <xs:complexType name="BCARQComplexType">
    <xs:sequence>
      <xs:element name="NomArq" type="Nome">
        <xs:annotation>
          <xs:documentation>
            Nome do Arquivo
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NumCtrlEmis" type="ControleIF">
        <xs:annotation>
          <xs:documentation>
            Número Controle do Emissor
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NumCtrlDestOr" type="ControleIF" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Número Controle Destinatário Original
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ISPBEmissor" type="CNPJBase">
        <xs:annotation>
          <xs:documentation>
            ISPB Emissor
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ISPBDestinatario" type="CNPJBase">
        <xs:annotation>
          <xs:documentation>
            ISPB Destinatário
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DtHrArq" type="DataHora">
        <xs:annotation>
          <xs:documentation>
            Data e Hora de envio
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SitReq" type="SitReq" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Situação Requisição
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Grupo_Seq" type="Grupo_SeqComplexType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Grupo Sequência
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DtRef" type="Data">
        <xs:annotation>
          <xs:documentation>
            Data de Referência
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Grupo_SeqComplexType">
    <xs:sequence>
      <xs:element name="NumSeq" type="NumSeq">
        <xs:annotation>
          <xs:documentation>
            Número Sequência
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IndrCont" type="Indr">
        <xs:annotation>
          <xs:documentation>
            Indicador Continuidade
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- Definição do BCARQ do arquivo ERR -->
  <xs:complexType name="BCARQERRComplexType">
    <xs:sequence>
      <xs:element name="NomArq" type="NomArqCodErr_ComplexType">
        <xs:annotation>
          <xs:documentation>
            Nome do Arquivo
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- Definição do ESTARQ do arquivo -->
  <xs:simpleType name="ESTARQSimpleType">
    <xs:annotation>
      <xs:documentation>
        Cópia do conteúdo das informacões de estímulo, quando houverem.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="512000"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- Definicao do USRMSG da mensagem -->

  <xs:simpleType name="USERMSGSimpleType">
    <xs:restriction base="xs:string">
      <xs:maxLength value="1048576"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- Definição do tipos simples e complexos do evento -->
  <xs:simpleType name="Ano">
    <xs:annotation>
      <xs:documentation>
        Formato: AAAA
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="(110.00|21)\d\d"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="AnoCodErr">
    <xs:annotation>
      <xs:documentation>
        Ano com código de erro
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="Ano">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="AnoMes">
    <xs:annotation>
      <xs:documentation>
        Formato = AAAA-MM, onde AAAA é ano – MM é mês
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="(110.00|21)\d\d-(0[1-9]|1[012])"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="AnoMesCodErr">
    <xs:annotation>
      <xs:documentation>
        Ano e mês com código de erro
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="AnoMes">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CNPJ">
    <xs:annotation>
      <xs:documentation>
        Número de Identificação da Pessoa Jurídica no
        Cadastro Nacional de Pessoa Jurídica. No caso de
        transferência para pessoas isentas de CNPJ,
        o campo CNPJ deve ser preenchido com zero.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{14}"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CNPJCodErr">
    <xs:annotation>
      <xs:documentation>
        Número de Identificação da Pessoa Jurídica no
        Cadastro Nacional de Pessoa Jurídica. No caso de
        transferência para pessoas isentas de CNPJ,
        o campo CNPJ deve ser preenchido com zero.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CNPJ">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CNPJBase">
    <xs:annotation>
      <xs:documentation>
        Primeiras 8 posicões do CNPJ.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="8"/>
      <xs:maxLength value="8"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CNPJBaseCodErr">
    <xs:annotation>
      <xs:documentation>
        CNPJ base com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CNPJBase">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="TpPessoaINTCodErr">
    <xs:annotation>
      <xs:documentation>
        Tipo Pessoa Intermediário
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="TpPessoaINT">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="TpPessoaSacdCodErr">
    <xs:annotation>
      <xs:documentation>
        Tipo Pessoa Sacado
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="TpPessoaSacd">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="NumBcoCodErr">
    <xs:annotation>
      <xs:documentation>
        Número do Banco
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="NumBco">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="DigtVerificdrCodErr">
    <xs:annotation>
      <xs:documentation>
        Digíto verificador
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="DigtVerificdr">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="LinhaDigitvlCodErr">
    <xs:annotation>
      <xs:documentation>
        Fim Grupo de Boletos
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="LinhaDigitvl">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CNPJ_CPF">
    <xs:annotation>
      <xs:documentation>
        Número de Identificação da Pessoa Jurídica no
        Cadastro Nacional de Pessoa Jurídica ou o Número
        no Cadastro de Pessoa Física. No caso de
        transferência para pessoas isentas de CNPJ ou
        CPF, o campo CNPJ ou CPF deve ser preenchido com zero.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{11}||\d{14}"/>
      <xs:minLength value="11"/>
      <xs:maxLength value="14"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CNPJ_CPFCodErr">
    <xs:annotation>
      <xs:documentation>
        Número de Identificacao da Pessoa Jurídica no Cadastro Nacional de Pessoa Jurídica
        ou o Número no Cadastro de Pessoa Física com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CNPJ_CPF">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CPF">
    <xs:annotation>
      <xs:documentation>
        Número no Cadastro de Pessoa Física. No caso de
        transferência para pessoas isentas de CNPJ ou
        CPF, o campo CPF deve ser preenchido com zero.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{11}"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CPFCodErr">
    <xs:annotation>
      <xs:documentation>
        Número no Cadastro de Pessoa Física. No caso de
        transferência para pessoas isentas de CNPJ ou
        CPF, o campo CPF deve ser preenchido com zero.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CPF">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CodCatg">
    <xs:annotation>
      <xs:documentation>
        Código da categoria do veículo
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="15"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CodCatgCodErr">
    <xs:annotation>
      <xs:documentation>
        Código da categoria do veículo
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodCatg">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CodContrtoOr">
    <xs:annotation>
      <xs:documentation>
        Código do contrato original, preenchidos com zeros à esquerda
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="40"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="CodContrtoOrCodErr">
    <xs:annotation>
      <xs:documentation>
        Código do contrato original com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodContrtoOr">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CodContrtoSCR">
    <xs:annotation>
      <xs:documentation>
        Código de identificação do contrato SCR
        Código alfanumérico interno da IF identificador do contrato.
        Deverá ser preenchido exatamente com o mesmo conteúdo que é informado no layout SCR 3040
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="40"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="CodContrtoSCRCodErr">
    <xs:annotation>
      <xs:documentation>

      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodContrtoSCR">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="RetenSubtlRsc">
    <xs:annotation>
      <xs:documentation>
        Identificador de Retenção Substancia de Risco.
        Deverá ser preenchido conforme o domínio “Cedente” ou “Cessionário”, obrigatório.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:totalDigits value="1"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="RetenSubtlRscErr">
    <xs:annotation>
      <xs:documentation>
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="RetenSubtlRsc">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CodErro">
    <xs:annotation>
      <xs:documentation>
        Código de erro. Vide tabela de código de erros no Manual de Leiautes.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="8"/>
      <xs:maxLength value="8"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CodInstntoFinanc">
    <xs:annotation>
      <xs:documentation>
        Código Instrumento Financeiro
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="12"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CodInstntoFinancCodErr">
    <xs:annotation>
      <xs:documentation>
        Código Instrumento Financeiro
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodInstntoFinanc">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>


  <xs:simpleType name="CodMarca">
    <xs:annotation>
      <xs:documentation>
        Código da marca do veículo
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="5"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CodMarcaCodErr">
    <xs:annotation>
      <xs:documentation>
        Código da marca do veículo
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodMarca">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CodModl">
    <xs:annotation>
      <xs:documentation>
        Código da modelo do veículo
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="8"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CodModlCodErr">
    <xs:annotation>
      <xs:documentation>
        Código da modelo do veículo
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodModl">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CodMoedaCNAB">
    <xs:annotation>
      <xs:documentation>
        <cat:InfTipo>
          <cat:DescricaoTipo>Código da moeda no padrão CNAB.</cat:DescricaoTipo>
        </cat:InfTipo>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="2"/>
      <xs:maxLength value="2"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="CodMoedaCNABCodErr">
    <xs:annotation>
      <xs:documentation>

      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodMoedaCNAB">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CodProdt">
    <xs:annotation>
      <xs:documentation>
        Código de Produto
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="2"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CodProdtCodErr">
    <xs:annotation>
      <xs:documentation>
        Código de Produto
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodProdt">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CodTabVeicl">
    <xs:annotation>
      <xs:documentation>
        Código de Produto
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="11"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CodTabVeiclCodErr">
    <xs:annotation>
      <xs:documentation>
        NCódigo de Produto com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodTabVeicl">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="ControleIF">
